#FUNCTION {max}
{
    #IF {%1 > %2}
    {
        #RETURN %1;
    }
    {
        #RETURN %2;
    };
}

#FUNCTION {min}
{
    #IF {%1 > %2}
    {
        #RETURN %1;
    }
    {
        #RETURN %2;
    };
}

#FUNCTION {clamp}
{
    #IF {%1 < %2}
    {
        #RETURN %2;
    };

    #IF {%1 > %3}
    {
        #RETURN %3;
    };

    #RETURN %1;
}

#FUNCTION {has_module}
{
    #LIST modules find %1 list_check;
    #LOCAL check $list_check;
    #UNVAR list_check;
    #RETURN $check;
}
